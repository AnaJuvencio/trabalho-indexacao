    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/T01/ORI_T01", // Caminho para o execut√°vel
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/T01",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
           "miDebuggerPath": "/usr/bin/gdb",

            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
        

    ]
}